generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Invitation {
  id        String   @id @default(uuid())
  slug      String   @unique
  guestName String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}